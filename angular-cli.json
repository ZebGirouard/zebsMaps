{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "angular-cli-project"
  },
  "apps": [
    {
      "root": "./",
      "outDir": "dist",
      "assets": [
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "app/main.ts",
      "test": "app/test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "node_modules/core-js/client/shim.min.js",
        "app/scripts/usMap.js",
        "node_modules/zone.js/dist/zone.js",
        "node_modules/systemjs/dist/system.src.js",
        "systemjs.config.js",
        "app/scripts/system.import.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.config.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}